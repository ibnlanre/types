{
  "name": "@ibnlanre/types",
  "version": "0.0.2",
  "description": "An opinionated collection of typescript types",
  "homepage": "https://github.com/ibnlanre/types#readme",
  "type": "module",
  "scripts": {
    "bundle": "tsup",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "test": "vitest -- --typecheck",
    "package": "yarn publish --access public"
  },
  "files": [
    "dist/*"
  ],
  "main": "cjs/index.cjs",
  "module": "esm/index.js",
  "exports": {
    "./*": "./dist/*"
  },
  "types": "esm/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "devDependencies": {
    "ts-arithmetic": "^0.1.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  },
  "author": "Ridwan Olanrewaju <olaitanlanre28@gmail.com>",
  "repository": {
    "url": "https://github.com/ibnlanre/types.git",
    "type": "git",
    "publishConfig": {
      "access": "public"
    }
  },
  "bugs": {
    "url": "https://github.com/ibnlanre/types/issues"
  },
  "license": "MIT",
  "keywords": [
    "types",
    "typescript",
    "type-fest"
  ]
}
